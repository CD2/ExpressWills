= render 'header'
%h4 Third Executor

= form_for [@will, @executor] do |f|
  = f.fields_for :third_executor_general_detail do |builder|
    = render "general_details/general_details_form", :builder => builder, :nested_origin => @executor.third_executor_general_detail
  .field
    = question "03050"
    = f.check_box :forth

  = f.submit 'Save', class: "button radius"